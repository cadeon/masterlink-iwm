<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
           "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
           "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
   <class name="org.mlink.iwm.hibentity.Customer"
      table="Customer"
      lazy="false">
      
      <id
            name="id"
            column="ID"
            type="java.lang.Long"
        >
            <generator class="sequence">
                <param name="sequence">job_seq</param>
            </generator>
        </id>
      
      <property name="fname"
         access="property"/>
      <property name="lname"
         access="property"/>
   </class>
</hibernate-mapping>
